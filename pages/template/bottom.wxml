<template name="bottom">
<view class="bottom">
  <view class="submit" bindtap="unlock" wx:if="{{ paysuccess == 0 || needReunlock == 1 }}">{{ title }}</view>
  <view class="submitSuccess" wx:if="{{ paysuccess === 1 }}">
    <image src="../image/paysuccess.png" style="width:40px;height:40px"/><text>  支付成功</text>
  </view>
</view>
</template>