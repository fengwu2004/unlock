  .bg {
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%; 
    position: relative;
    width: 100%;
  }

  .card {
    position: relative;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%; 
    width: 80%;
    left: 0;
    right: 0;
    top: 20px;
    margin: auto;
  }

  .card > view {

    padding-left: 30px;
    padding-right: 30px;
  }

  .title {

    line-height: 50px;
    height: 50px;
    padding-top: 15px;
  }

  .title > image {

    width: 25px;
    height: 25px;
    vertical-align: middle;
  }

  .title > text {

    padding-left: 20px;
    line-height: 50px;
    vertical-align: middle;
  }

  .avatar {
    right: -10px;
    top: 10px;
    width: 60px;
    height: 60px;
    position: absolute;
    text-align: center;
  }

  .avatar > image {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
  }